class Taxi :
    def __init__ (self, marca, cor, modelo, placa, ano) :
        self._marca = marca
        self._cor = cor
        self._modelo = modelo
        self._placa = placa
        self._ano = ano
    
    def set_marca (self) :
        self._marca
    def get_marca (self) :
        return self._marca
    def set_cor (self) :
        self._cor
    def get_cor (self) :
        return self._cor
    def set_modelo (self) :
        self._modelo
    def get_modelo (self) :
        return self._modelo
    def set_placa (self) :
        self._placa
    def get_placa (self) :
        return self._placa
    def set_ano (self) :
        self._ano
    def get_ano (self) :
        return self._ano
