
class Cliente :
    
    def __init__(self, nome,codigo):
        self._nome = nome
        self._codigo = codigo
    
    def set_nome (self):
        self._nome 
    
    def get_nome(self):
        return self._nome 

    def set_codigo (self):
        self._codigo 
    
    def get_codigo(self):
        return self._codigo
